{
  "name": "Search Discogs for selected text",
  "description": "Extension enables you to search on Discogs by text selected on a page",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "contextMenu.js"
  },
  "permissions": ["contextMenus"],
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  }
}
